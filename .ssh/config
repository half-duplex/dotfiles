# https://stribika.github.io/2015/01/04/secure-secure-shell.html
# ssh-keygen -t ed25519 -o -a 100 -f ~/.ssh/keys/id_ed25519_x
# ssh-keygen -t rsa -b 4096 -o -a 100 -f ~/.ssh/keys/id_rsa_x

Host eva
    HostName eva.sec.gd
    IdentityFile /home/mal/.ssh/keys/id_ed25519_eva
Host edge
    HostName 10.0.0.1
    Port 23
    IdentityFile /home/mal/.ssh/keys/id_ed25519_edge
Host luca
    HostName luca.sec.gd
    IdentityFile /home/mal/.ssh/keys/id_rsa
Host blake
    HostName blake.sec.gd
    IdentityFile /home/mal/.ssh/keys/id_rsa

Host pluto
    HostName pluto.sec.gd
    IdentityFile /home/mal/.ssh/keys/id_ed25519_pluto
Host fde.pluto.sec.gd
    HostName pluto.sec.gd
    AddressFamily inet
    User root
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-ripemd160-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha1
    HostKeyAlias fde-pluto
    IdentityFile /home/mal/.ssh/keys/id_rsa_pluto_fde

Host nova
    HostName nova.sec.gd
    IdentityFile /home/mal/.ssh/keys/id_ed25519_nova
Host fde.nova.sec.gd
    HostName nova.sec.gd
    AddressFamily inet
    User root
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-ripemd160-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha1
    HostKeyAlias fde-nova
    IdentityFile /home/mal/.ssh/keys/id_rsa_nova_fde

Host ctf
    HostName 10.194.0.45
    IdentityFile /home/mal/.ssh/keys/id_rsa_ctf365
Host ctfp
    HostName eva.sec.gd
    Port 23
    IdentityFile /home/mal/.ssh/keys/id_rsa_ctf365

Host bastion
    HostName bastion.foodforsecurity.com
    Port 1991
    IdentityFile /home/mal/.ssh/keys/id_rsa
Host tron
    HostName tron.disruptivelabs.org
    IdentityFile /home/mal/.ssh/keys/id_rsa

Host github.com
    IdentityFile /home/mal/.ssh/keys/id_rsa
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-ripemd160-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512
Host bitbucket.org
    IdentityFile /home/mal/.ssh/keys/id_rsa
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-ripemd160-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-ripemd160
Host *.onion
    ProxyCommand socat - SOCKS4A:localhost:%h:%p,socksport=9050
Host *
    PubkeyAuthentication yes
    HostKeyAlgorithms ssh-ed25519-cert-v01@openssh.com,ssh-rsa-cert-v01@openssh.com,ssh-ed25519,ssh-rsa
    KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256
    Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
    MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,hmac-ripemd160-etm@openssh.com,umac-128-etm@openssh.com

