! helpful: http://www.cssdrive.com/imagepalette/index.php
! helpful: http://www.colorhexa.com/

*background: #252525
*foreground: #90b3b3

! x;3y;4z x=style y=fg (colory) z=bg (colorz+8)
! Black + DarkGrey
*color0:  #5b5b5b
*color8:  #737373
! DarkRed + Red
*color1:  #9c748f
*color9:  #c392b3
! DarkGreen + Green
*color2:  #615375
*color10: #7a6994
! DarkYellow + Yellow
*color3:  #5d78ab
*color11: #7496d6
! DarkBlue + Blue
*color4:  #31548f
*color12: #3d69b3
! DarkMangenta + Mangenta
*color5:  #6264e3
*color13: #6e70ff
! DarkCyan + Cyan
*color6:  #1f91cc
*color14: #27b5ff
! LightGrey + White
*color7:  #90b3b3
*color15: #b3dede

*utf8: 1

URxvt*scrollBar:        false
URxvt*jumpScroll:       true
URxvt*scrollTtyOutput:  false
URxvt*scrollTtyKeypress: true
URxvt*mouseWheelScrollPage: false
URxvt.font:             6x13
!URxvt.font:             xft:Liberation Mono:size=8:antialias=true
URxvt*iso14755:         False

URxvt.perl-ext-common: default,matcher,clipboard
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

